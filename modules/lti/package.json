      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "http-mock-server": "^0.5.2",
    "@types/jest": "26.0.21",
    "@types/node": "14.14.35",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@types/react-js-pagination": "^3.0.3",
    "@types/react-select": "^3.0.21"
